{
  "name": "typo3-ter/beautyofcode",
  "type": "typo3-cms-extension",
  "description": "Provides state-of-the-art syntax highlighting by using JavaScript.",
  "keywords": [
    "TYPO3",
    "extension",
    "syntax highlighting",
    "prism",
    "syntaxhighlighter"
  ],
  "homepage": "https://github.com/fnagel/beautyofcode",
  "authors": [
    {
      "name": "Felix Nagel",
      "email": "info@felixnagel.com",
      "role": "Project Leader, Developer"
    },
    {
      "name": "Thomas Juhnke",
      "email": "dev@van-tomas.de",
      "role": "Project Co-Leader, Developer"
    }
  ],
  "license": "GP-2.0+",
  "require": {
    "php": ">=7.0,<7.3",
    "typo3/cms-core": ">=8.7.0,<=9.5.99"
  },
  "require-dev": {
    "typo3/testing-framework": "*",
    "fluidtypo3/development": "*"
  },
  "autoload": {
    "psr-4": {
      "TYPO3\\Beautyofcode\\": "Classes"
    }
  },
  "autoload-dev": {
    "files": [
      "./class.ext_update.php"
    ]
  }
}
