  "keywords": [
    "TYPO3",
    "extension",
    "syntax highlighting",
    "prism",
    "syntaxhighlighter"
  ],
  "homepage": "https://github.com/fnagel/beautyofcode",
  "authors": [
    {
      "name": "Felix Nagel",
      "email": "info@felixnagel.com",
      "role": "Project Leader, Developer"
    },
    {
      "name": "Thomas Juhnke",
      "email": "dev@van-tomas.de",
      "role": "Project Co-Leader, Developer"
    }
  ],
  "license": "GP-2.0+",
  "require": {
    "php": ">=5.4.0,<7.1",
    "typo3/cms-core": "^7.3.1 || ^8.0"
  },
  "require-dev": {
    "fluidtypo3/development": "*"
  },
  "autoload": {
    "psr-4": {
      "TYPO3\\Beautyofcode\\": "Classes"
    }
  },
  "autoload-dev": {
    "files": [
      "./class.ext_update.php"
    ]
  }
